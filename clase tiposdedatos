public class Tipos1 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
       
        byte m=2;
        short n=2;
        int o=2;
        long p=2;
        
        m=(byte)p;
        m=(byte)o;
        p=m;
        n=(short)p;
        p=o;
        o=n;
        o=(int)p;
        n=m;
        n=(short)o;
        o=m;
        
    }
    
}
